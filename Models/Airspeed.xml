<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>Airspeed.ac</path>

  <animation>
    <type>noshadow</type>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>Needle</object-name>
    <min-m>0</min-m>
    <max-m>3</max-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>Face</object-name>
    <min-m>0</min-m>
    <max-m>10</max-m>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry>
        <ind> 0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind> 40</ind>
        <dep> 27.69</dep>
      </entry>
      <entry>
        <ind>260</ind>
        <dep>332.3</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
